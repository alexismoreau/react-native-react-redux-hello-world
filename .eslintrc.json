{
  "env": {
    "node": true,
    "browser": true,
    "es6": true
  },
  "parser": "babel-eslint",
  "extends": "airbnb",
  "rules": {
    "indent": [2, 2],
    "global-require": 0,
    "import/no-extraneous-dependencies": 0,
    "react/jsx-filename-extension": ["error", { "extensions": [".js", ".jsx"] }]
  }
}
